{
  "name": "stripe-e-commerce",
  "version": "0.0.0",
  "author": "Mark Watson <mark@watsonised.com> (https://get.watsonised.com)",
  "description": "stripe e-commerce netlify serverless in react with vite server",
  "license": "MIT",
  "private": true,
  "type": "module",
  "keywords": [
    "vite",
    "sass",
    "react",
    "stripe"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run prebuild && vite build",
    "prebuild": "rm -rf dist/ && rm -rf build/",
    "preview": "vite preview",
    "vitest-preview": "vitest-preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:debug": "npm-run-all -p test vitest-preview",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^3.6.0",
    "@stripe/stripe-js": "^7.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.0",
    "stripe": "^18.0.0"
  },
  "devDependencies": {
    "@jest/expect-utils": "^29.7.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vis.gl/react-google-maps": "^1.5.2",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "jest-matcher-utils": "^29.7.0",
    "jsdom": "^26.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.86.3",
    "vite": "^6.2.5",
    "vite-plugin-react": "^4.0.1",
    "vitest": "^3.1.1",
    "vitest-preview": "^0.0.1"
  }
}
